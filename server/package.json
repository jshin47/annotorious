{
  "name": "annotorious-server",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm-run-all --parallel start playground",
    "start": "nodemon --inspect -e ts,graphql -x ts-node -r dotenv/config src/index.ts",
    "playground": "graphql playground",
    "build": "rm -rf dist && graphql codegen && tsc",
    "prisma": "prisma",
    "resolver-interfaces": "graphql-resolver-codegen interfaces -f -s src/schema.graphql -o ./src/generated/resolvers.ts",
    "resolver-scaffold": "graphql-resolver-codegen scaffold -f -s src/schema.graphql -o ./src/resolvers/ -i ../generated/resolvers",
    "resolver-codegen": "npm-run-all resolver-interfaces resolver-scaffold",
    "codegen": "graphql codegen",
    "regen": "prisma deploy && yarn resolver-codegen && yarn codegen && prisma generate"
  },
  "resolutions": {
    "graphql": "0.13.2"
  },
  "dependencies": {
    "apollo-server-express": "^2.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^4.0.0",
    "graphql-yoga": "^1.16.2",
    "jsonwebtoken": "^8.3.0",
    "passport": "^0.4.0",
    "prisma-binding": "^2.1.6",
    "prisma-client-lib": "^1.17.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "dotenv": "^6.0.0",
    "graphql-cli": "^2.16.7",
    "graphql-resolver-codegen": "^0.2.4",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prisma": "^1.17.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1"
  }
}
